<h1>New Itinerary</h1>
<%= form_for @trip do |f| %>
	<div>
	    <%= f.label :location %>
	    <%= f.text_field :location %>
    </div>

    <div>
	    <%= f.label :starting_time %>
	    <%= f.text_field :starting_time %>
  	</div>

  	<div>
	    <%= f.label :ending_time %>
	    <%= f.text_field :ending_time %>
  	</div>
  	<div>
	    <%= f.label :experiences %>
	    <%= f.text_field :experiences %>
  	</div>
  	<div>
    	<%= f.submit %>
  	</div>
<% end %>
