<div> 
<h1>Users</h1>
<div ng-app= "dayPilotApp" ng-controller="DayPilotCtrl">
	<div class="col-md-3"></div>
	<div class="col-md-9">
		<daypilot-calendar id="week" daypilot-config="daysConfig"></daypilot-calendar>
	</div>
	{{foo}}
</div>
<ul>
  <% @users.each do |user| %>
    <li><%= user.first %> <%= user.last %> <%= link_to "View", user_path(user)  %> <%= link_to "Edit", edit_user_path(user)  %> <%= link_to "Delete", user_path(user), method: :delete, data: { confirm: "Are you sure you want to delete this user?"} %> </li>
  <% end %>
</ul>
</div>
