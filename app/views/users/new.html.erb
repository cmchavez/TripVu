<% else %>
<h1>Sign Up</h1>
<%= form_for @user do |f| %>
  <div>
      <%= f.label :first, "First Name:"%>
      <%= f.text_field :first %>
  </div>
  <div>
    <%= f.label :last, "Last Name:" %>
    <%= f.text_field :last %>
  </div>

  <div>
    <%= f.label :email, "Email:" %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :home, "Home:" %>
    <%= f.text_field :home %>
  </div>
  <div>
    <%= f.label :age, "Age:" %>
    <%= f.text_field :age %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
