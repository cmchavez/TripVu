
<h2>Go to: <%= link_to 'Users', users_path  %>  <%#= link_to 'Trips', trips_path  %> </h2>
<% else %>
<h1>Sign Up</h1>
<%= form_for @user do |f| %>
<div>
    <%= f.label :first %>
    <%= f.text_field :first %>
    <%= f.label :last %>
    <%= f.text_field :last %>
  </div>

  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :home %>
    <%= f.text_field :home %>
  </div>
  <div>
    <%= f.submit %>
  </div>
<% end %>
